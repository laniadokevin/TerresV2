@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>Register</h1>
<form method="post">
    <div>
        <label>Email:</label>
        <input type="email" name="email" required />
    </div>
    <div>
        <label>Password:</label>
        <input type="password" name="password" required />
    </div>
    <button type="submit">Register</button>
</form>